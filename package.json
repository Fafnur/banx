    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "jest-preset-angular": "^9.0.7",
    "mysql2": "^2.3.0",
    "ng-mocks": "^12.4.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "ts-jest": "^27.0.5",
    "ts-mockito": "^2.6.1",
    "ts-node": "~10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typeorm": "^0.2.37",
    "typescript": "~4.3.5"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "resolutions": {
    "rxjs": "^6.6.7"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pretty-quick --staged",
      "pre-push": "yarn affected:lint && yarn affected:test"
    }
  }
}
