    "@nrwl/cli": "^12.8.0",
    "@nrwl/cypress": "^12.8.0",
    "@nrwl/eslint-plugin-nx": "^12.8.0",
    "@nrwl/jest": "^12.8.0",
    "@nrwl/linter": "^12.8.0",
    "@nrwl/nest": "^12.8.0",
    "@nrwl/node": "^12.8.0",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.7.6",
    "@types/passport-jwt": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "bcrypt": "^5.0.1",
    "cypress": "^7.7.0",
    "dotenv": "^10.0.0",
