{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "yarn"
  },
  "defaultProject": "russian-cabinet",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "russian-cabinet": {
      "projectType": "application",
      "root": "apps/russian/cabinet",
      "sourceRoot": "apps/russian/cabinet/src",
      "prefix": "banx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/russian/cabinet",
            "index": "apps/russian/cabinet/src/index.html",
            "main": "apps/russian/cabinet/src/main.ts",
            "polyfills": "apps/russian/cabinet/src/polyfills.ts",
            "tsConfig": "apps/russian/cabinet/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/russian/cabinet/src/favicon.ico", "apps/russian/cabinet/src/assets"],
            "styles": ["apps/russian/cabinet/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/russian/cabinet/src/environments/environment.ts",
                  "with": "apps/russian/cabinet/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "russian-cabinet:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "russian-cabinet:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "russian-cabinet:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/russian/cabinet/src/**/*.ts", "apps/russian/cabinet/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/russian/cabinet"],
          "options": {
            "jestConfig": "apps/russian/cabinet/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "russian/cabinet-e2e": {
      "root": "apps/russian/cabinet-e2e",
      "sourceRoot": "apps/russian/cabinet-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/russian/cabinet-e2e/cypress.json",
            "tsConfig": "apps/russian/cabinet-e2e/tsconfig.e2e.json",
            "devServerTarget": "russian-cabinet:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "russian-cabinet:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/russian/cabinet-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "russian-ui-styles": {
      "projectType": "library",
      "root": "libs/russian/ui/styles",
      "sourceRoot": "libs/russian/ui/styles/src",
      "prefix": "banx",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/russian/ui/styles/src/**/*.ts", "libs/russian/ui/styles/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/russian/ui/styles"],
          "options": {
            "jestConfig": "libs/russian/ui/styles/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "russian-ui-layout": {
      "projectType": "library",
      "root": "libs/russian/ui/layout",
      "sourceRoot": "libs/russian/ui/layout/src",
      "prefix": "banx",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/russian/ui/layout/src/**/*.ts", "libs/russian/ui/layout/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/russian/ui/layout"],
          "options": {
            "jestConfig": "libs/russian/ui/layout/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "russian-ui-container": {
      "projectType": "library",
      "root": "libs/russian/ui/container",
      "sourceRoot": "libs/russian/ui/container/src",
      "prefix": "banx",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/russian/ui/container/src/**/*.ts", "libs/russian/ui/container/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/russian/ui/container"],
          "options": {
            "jestConfig": "libs/russian/ui/container/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
